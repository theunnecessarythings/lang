fn expr_test() f64 {
    var r: f64 = 2.0;
    var b: f64 = 3.14;
    var area: f64 = r * r * b;
    return area;
}

fn fib(n: i32) i32 {
    if n == 0 {
        return 0;
    }
    if n == 1 {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}

fn main() void {
    var area = expr_test();
    var n = fib(30);
    print("fib(%d) = %d", 30, n);
    print("area = %f", area);
}
